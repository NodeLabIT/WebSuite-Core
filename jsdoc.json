{
	"plugins": [],
	"recurseDepth": 10,
	"source": {
		"include": [
			"./core",
			"./index.js"
		],
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc","closure"]
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false,
		"default": {
			"outputSourceFiles": false
		}
	},
	"opts": {
		"destination": "./docs/out/",
		"readme": "./README.md",
		"recurse": true,
		"template": "../JSDoc-Theme",
		"tutorials": "./docs/tutorials",
		"mainpagetitle": "WebSuite Dokumentation"
	}
}