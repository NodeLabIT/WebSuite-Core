{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            "tab"
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ]
    },
    "globals": {
        "WebSuite": false,
        "CryptoUtil": false,
        "DirectoryUtil": false,
        "FileUtil": false,
        "TimeUtil": false,
        "UserUtil": false,
        "_dir": false
    }
}